{
   "name": "rescript-project-template",
   "sources": {
     "dir": "src",
     "subdirs": true
   },
   "package-specs": {
     "module": "commonjs",
     "in-source": true
   },
   "suffix": ".bs.js",
   "bs-dependencies": [
      "@rescript/core"
   ],
   "bsc-flags": [
      "-open RescriptCore"
   ],
   "gentypeconfig": {
     "language": "typescript",
     "generatedFileExtension": ".gen.tsx",
     "module": "es6",
     "shims": { 
      "Js": "Js",
      "RescriptCore": "RescriptCore" 
   }
   }
 }